BPChecker:addButton(
    "Bp Check",
    function()
        
local BPCount = 0

for i,v in next, game:GetService("Workspace").Effects.StructureModel:GetChildren() do
if v.Name == "Square" or v.Name == "OriginSquare" then
BPCount = BPCount + 1
end
end

if BPCount > 0 then
game.ReplicatedStorage.Notices.SendUserNotice:Fire("You have "..BPCount.." Blueprints")
end
    end
)
