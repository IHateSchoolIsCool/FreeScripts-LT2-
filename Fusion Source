                        KeybindValue.Parent = BindHolder
                        KeybindValue.BackgroundColor3 = theme[2]
                        KeybindValue.BorderSizePixel = 0
                        KeybindValue.Position = UDim2.new(0.735000014, 0, 0.111000001, 0)
