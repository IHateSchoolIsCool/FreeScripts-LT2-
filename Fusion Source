                -- // Namecall Hook \\ --
                OldNamecall = hookfunction(GameMT.__namecall, function(...)
                    local Arguments = {...}
                    local NCMethod = getnamecallmethod()
                    
                    if NCMethod == 'InvokeServer' and Arguments[1].Name == 'RequestSave' and getgenv().BlockSave then    
                        return true
                    end
                    return OldNamecall(...)
                end)
            end
            --#endregion

