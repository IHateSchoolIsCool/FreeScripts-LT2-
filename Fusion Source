            OldNewIndex = hookfunction(GameMT.__newindex, function(...)
                local Arguments = {...}
                if Arguments[2] == 'WalkSpeed' and not checkcaller() then
                    return OldNewIndex(({...})[1], ({...})[2], game.Players.LocalPlayer.Character.Humanoid.WalkSpeed)
                end
                return OldNewIndex(...)
            end)]]
